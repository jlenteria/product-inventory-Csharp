<?xml version="1.0" encoding="utf-8"?>
<Window
	x:Class="Inventory_Project.Window1" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	Title="Inventory_Project"
	Height="785"
	Width="950"
	WindowStyle="ToolWindow"
	WindowStartupLocation="CenterScreen"
	ResizeMode="NoResize">
	<Grid>
		<Image
			Source="Properties\backnew.jpg"
			Margin="0,10,0,-7"
			VerticalAlignment="Stretch"
			HorizontalAlignment="Stretch"
			Grid.Row="0"
			Grid.Column="0" />
		<StackPanel
			Width="210"
			Height="659"
			Grid.Column="0"
			Grid.Row="0"
			HorizontalAlignment="Left"
			VerticalAlignment="Top"
			Margin="5,75,0,0">
			<Menu
				DockPanel.Dock="Left"
				Height="150"
				Background="Transparent">
				<MenuItem
					Click="dash_Click"
					FontSize="0.1"
					Name="dashboard"
					Header="Dashboard"
					Width="209"
					Margin="0,10,0,0"
					Height="140"></MenuItem>
			</Menu>
			<Menu
				DockPanel.Dock="Left"
				Height="150"
				Margin="0,30,0,0"
				Background="Transparent">
				<MenuItem
					FontSize="0.1"
					Name="pinventory"
					Header="Perform Inventory"
					Width="209"
					Height="140"
					Margin="0,-1,0,0">
					<MenuItem
						Click="purchase_Click"
						Name="purchase"
						Header="Purchase"
						FontSize="13">
						<MenuItem.Icon>
							<Image
								Source="Properties\purchase.png" />
						</MenuItem.Icon>
					</MenuItem>
					<MenuItem
						Click="sell_Click"
						Name="sell"
						Header="Sell"
						FontSize="13">
						<MenuItem.Icon>
							<Image
								Source="Properties\sell.png" />
						</MenuItem.Icon>
					</MenuItem>
					<MenuItem
						Click="return_Click"
						Name="returnn"
						Header="Return"
						FontSize="13">
						<MenuItem.Icon>
							<Image
								Source="Properties\return.png" />
						</MenuItem.Icon>
					</MenuItem>
					<MenuItem
						Click="dispose_Click"
						Name="dispose"
						Header="Dispose"
						FontSize="13">
						<MenuItem.Icon>
							<Image
								Source="Properties\dispose.png" />
						</MenuItem.Icon>
					</MenuItem>
				</MenuItem>
			</Menu>
			<Menu
				DockPanel.Dock="Left"
				Height="150"
				Margin="0,18,0,0"
				Background="Transparent">
				<MenuItem
					Name="vinventory"
					FontSize="0.1"
					Header="View Inventory"
					Width="209"
					Height="142"
					Margin="0,-1,0,0">
					<MenuItem
						Click="view_product_click"
						Name="view_product"
						Header="View all Product"
						FontSize="13" />
					<MenuItem
						Click="view_category_click"
						Name="view_category"
						Header="View by Category"
						FontSize="13" />
					<MenuItem
						Click="view_status_click"
						Name="view_status"
						Header="View by Status"
						FontSize="13" />
				</MenuItem>
			</Menu>
			<Menu
				Name="updateinventory"
				Background="Transparent"
				DockPanel.Dock="Left"
				Margin="0,10,0,0">
				<MenuItem
					Click="update_Click"
					Name="uinventory"
					FontSize="0.1"
					Header="Update Inventory"
					Width="209"
					Height="150"
					Margin="0,1,0,0"></MenuItem>
			</Menu>
		</StackPanel>
		<StackPanel
			Width="695"
			Height="490"
			Grid.Column="0"
			Grid.Row="0"
			HorizontalAlignment="Right"
			VerticalAlignment="Top"
			Margin="0,255,10,0">
			<!-- Perform Inventory -->
			<TabControl
				Background="Lavender"
				Name="perform_inventory"
				Margin="0,5,0,0"
				VerticalAlignment="Stretch"
				HorizontalAlignment="Center"
				Grid.Row="0"
				Grid.Column="0"
				Height="510"
				Width="680">
				<TabItem
					Name="products"
					Width="131"
					Background="DarkGray"
					Height="52"
					Margin="-5,-2,-2,0">
					<TabItem.Header>
						<Label
							Content="PRODUCTS" />
					</TabItem.Header>
					<StackPanel
						Margin="0,-35,0,0"
						Height="400">
						<Grid>
							<ScrollViewer
								HorizontalScrollBarVisibility="Auto"
								VerticalScrollBarVisibility="Auto">
								<DataGrid
									Name="Supplier_Products"
									AutoGenerateColumns="False"
									CanUserAddRows="False"
									Margin="0,5,0,0"
									Height="400"
									Width="680"
									VerticalAlignment="Center">
									<DataGrid.Columns>
										<DataGridTemplateColumn
											Header="Image"
											Width="100">
											<DataGridTemplateColumn.CellTemplate>
												<DataTemplate>
													<Image
														Source="{Binding Image}"
														Height="80"
														Width="150" />
												</DataTemplate>
											</DataGridTemplateColumn.CellTemplate>
										</DataGridTemplateColumn>
										<DataGridTextColumn
											Header="Name"
											Width="Auto"
											Binding="{Binding Name }" />
										<DataGridTextColumn
											Header="Id Number"
											Width="Auto"
											Binding="{Binding IdNo}" />
										<DataGridTextColumn
											Header="Category"
											Width="Auto"
											Binding="{Binding Category}" />
										<DataGridTextColumn
											Header="Total Quantity"
											Width="Auto"
											Binding="{Binding TotalQuantity}" />
										<DataGridTextColumn
											Header="Available Quantity"
											Width="Auto"
											IsReadOnly="True"
											Binding="{Binding Quantity}" />
										<DataGridTextColumn
											Header="Brand"
											Width="Auto"
											Binding="{Binding Brand}" />
										<DataGridTextColumn
											Header="Supplier"
											Width="Auto"
											Binding="{Binding Supplier}" />
										<DataGridTextColumn
											Header="Original Price/peso"
											Width="Auto"
											Binding="{Binding OriginalPrice}" />
										<DataGridTextColumn
											Header="Date of Expiration"
											Width="Auto"
											Binding="{Binding DateExpiration}" />
										<DataGridTextColumn
											Header="Retail Price"
											Width="Auto"
											Binding="{Binding RetailPrice/peso}" />
										<DataGridTextColumn
											Header="Status"
											Width="Auto"
											Binding="{Binding Status}" />
										<DataGridTextColumn
											Header="Date Of Order"
											Width="Auto"
											Binding="{Binding DateofOrder}" />
										<DataGridTextColumn
											Header="Description"
											Width="Auto"
											Binding="{Binding Description}" />
									</DataGrid.Columns>
									<DataGrid.ItemContainerStyle>
										<Style
											TargetType="{x:Type DataGridRow}">
											<EventSetter
												Event="MouseDoubleClick"
												Handler="DataGridItem_click" />
										</Style>
									</DataGrid.ItemContainerStyle>
								</DataGrid>
							</ScrollViewer>
						</Grid>
					</StackPanel>
				</TabItem>
				<TabItem
					Background="DarkGray"
					Height="50"
					Width="131"
					x:Name="selected"
					Margin="-5,-2,-2,0">
					<StackPanel
						Margin="0,-30,0,0"
						Height="370">
						<ScrollViewer
							HorizontalScrollBarVisibility="Auto"
							VerticalScrollBarVisibility="Auto"
							Height="367">
							<DataGrid
								Name="Purchase_Product"
								AutoGenerateColumns="False"
								CanUserAddRows="False"
								Width="650"
								Height="290"
								Margin="0,-75,0,0">
								<DataGrid.Columns>
									<DataGridTemplateColumn
										Header="Image"
										Width="100">
										<DataGridTemplateColumn.CellTemplate>
											<DataTemplate>
												<Image
													Source="{Binding Image, Mode=TwoWay}"
													Height="80"
													Width="150" />
											</DataTemplate>
										</DataGridTemplateColumn.CellTemplate>
									</DataGridTemplateColumn>
									<DataGridTextColumn
										Header="Name"
										Width="Auto"
										Binding="{Binding Name,Mode =TwoWay}"
										IsReadOnly="True" />
									<DataGridTextColumn
										Header="Id Number"
										Width="Auto"
										Binding="{Binding IdNo}" />
									<DataGridTextColumn
										Header="Category"
										Width="Auto"
										Binding="{Binding Category}" />
									<DataGridTextColumn
										Header="Total Quantity Purchased"
										Width="Auto"
										Binding="{Binding totalpurchased}" />
									<DataGridTextColumn
										Header="Original Price/peso"
										Width="Auto"
										Binding="{Binding OriginalPrice}" />
									<DataGridTextColumn
										Header="Total Purchase Price/Peso"
										Width="Auto"
										Binding="{Binding Totalprice}" />
									<DataGridTextColumn
										Header="Brand"
										Width="Auto"
										Binding="{Binding Brand}" />
									<DataGridTextColumn
										Header="Supplier"
										Width="Auto"
										Binding="{Binding Supplier}" />
									<DataGridTextColumn
										Header="Date of Expiration"
										Width="Auto"
										Binding="{Binding DateExpiration}" />
									<DataGridTextColumn
										Header="Retail Price"
										Width="Auto"
										Binding="{Binding RetailPrice/peso}" />
									<DataGridTextColumn
										Header="Status"
										Width="Auto"
										Binding="{Binding Status}" />
									<DataGridTextColumn
										Header="Date Of Order"
										Width="Auto"
										Binding="{Binding DateofOrder}" />
									<DataGridTextColumn
										Header="Description"
										Width="Auto"
										Binding="{Binding Description}" />
								</DataGrid.Columns>
							</DataGrid>
						</ScrollViewer>
						<Grid>
							<Button
								Click="confirm_Click"
								Content="Confirm Purchase"
								Grid.Column="0"
								Grid.Row="0"
								HorizontalAlignment="Stretch"
								VerticalAlignment="Stretch"
								Margin="300,-70,247,41" />
							<Button
								Click="remove_Click"
								Content="Remove Selection"
								Grid.Column="0"
								Grid.Row="0"
								HorizontalAlignment="Stretch"
								VerticalAlignment="Stretch"
								Margin="130,-70,375,41" />
							<Button
								Click="clear_Click"
								Content="Clear Selection"
								Grid.Column="0"
								Grid.Row="0"
								HorizontalAlignment="Stretch"
								VerticalAlignment="Stretch"
								Margin="428,-70,126,41" />
						</Grid>
					</StackPanel>
					<TabItem.Header>
						<Label
							Content="Selected Product" />
					</TabItem.Header>
				</TabItem>
			</TabControl>
			<!-- sell Inventory -->
			<TabControl
				Background="Lavender"
				Name="sell_inventory_tab"
				Margin="0,-540,0,0"
				VerticalAlignment="Stretch"
				HorizontalAlignment="Center"
				Grid.Row="0"
				Grid.Column="0"
				Height="510"
				Width="680">
				<TabItem
					Name="sell_item"
					Width="131"
					Background="DarkGray"
					Height="52">
					<TabItem.Header>
						<Label
							Content="INVENTORY" />
					</TabItem.Header>
					<ScrollViewer
						HorizontalScrollBarVisibility="Auto"
						VerticalScrollBarVisibility="Auto">
						<Grid>
							<ListView>
								<ListView.View>
									<GridView>
										<GridViewColumn
											Header="Image"
											Width="100" />
										<GridViewColumn
											Header="Name"
											Width="230" />
										<GridViewColumn
											Header="Id Number"
											Width="100" />
										<GridViewColumn
											Header="Total Quantity"
											Width="80" />
										<GridViewColumn
											Header="Brand"
											Width="130" />
										<GridViewColumn
											Header="Status"
											Width="100" />
										<GridViewColumn
											Header="Category"
											Width="150" />
										<GridViewColumn
											Header="Available Quantity"
											Width="100" />
										<GridViewColumn
											Header="Supplier"
											Width="220" />
										<GridViewColumn
											Header="Original Price"
											Width="150" />
										<GridViewColumn
											Header="Retail Price"
											Width="100" />
										<GridViewColumn
											Header="Date of Expiration"
											Width="150" />
										<GridViewColumn
											Header="Date of Order"
											Width="150" />
										<GridViewColumn
											Header="Description"
											Width="1000" />
									</GridView>
								</ListView.View>
							</ListView>
							<ListBox
								Name="Sell_Inventory"
								Margin="0,20,0,0">
								<ListBox.ItemTemplate>
									<DataTemplate>
										<Grid>
											<Grid.ColumnDefinitions>
												<ColumnDefinition
													Width="100" />
												<ColumnDefinition
													Width="250" />
												<ColumnDefinition
													Width="100" />
												<ColumnDefinition
													Width="100" />
												<ColumnDefinition
													Width="100" />
												<ColumnDefinition
													Width="100" />
												<ColumnDefinition
													Width="160" />
												<ColumnDefinition
													Width="70" />
												<ColumnDefinition
													Width="220" />
												<ColumnDefinition
													Width="200" />
												<ColumnDefinition
													Width="80" />
												<ColumnDefinition
													Width="150" />
												<ColumnDefinition
													Width="180" />
												<ColumnDefinition
													Width="Auto" />
											</Grid.ColumnDefinitions>
											<ListBoxItem>
												<StackPanel
													Orientation="Horizontal">
													<Image
														Grid.Column="0"
														Grid.Row="0"
														Source="{Binding Image, Mode=TwoWay}"
														Height="80"
														Width="80" />
												</StackPanel>
											</ListBoxItem>
											<Label
												Margin="5,10,0,0"
												Grid.Row="0"
												Grid.Column="1"
												Width="250"
												Content="{Binding Name, Mode=TwoWay}"
												FontSize="15" />
											<Label
												Margin="5,10,0,0"
												Grid.Row="0"
												Grid.Column="2"
												Width="150"
												Content="{Binding IdNo, Mode=TwoWay}"
												FontSize="15" />
											<Label
												Margin="5,10,0,0"
												Grid.Row="0"
												Grid.Column="3"
												Width="150"
												Content="{Binding TotalQuantity, Mode=TwoWay}"
												FontSize="15" />
											<Label
												Margin="5,10,0,0"
												Grid.Row="0"
												Grid.Column="4"
												Width="150"
												Content="{Binding Brand, Mode=TwoWay}"
												FontSize="15" />
											<Label
												Margin="5,10,0,0"
												Grid.Row="0"
												Grid.Column="5"
												Width="150"
												Content="{Binding Status, Mode=TwoWay}"
												FontSize="15" />
											<Label
												Margin="5,10,0,0"
												Grid.Row="0"
												Grid.Column="6"
												Width="150"
												Content="{Binding Category, Mode=TwoWay}"
												FontSize="15" />
											<Label
												Margin="5,10,0,0"
												Grid.Row="0"
												Grid.Column="7"
												Width="150"
												Content="{Binding Quantity, Mode=TwoWay}"
												FontSize="15" />
											<Label
												Margin="5,10,0,0"
												Grid.Row="0"
												Grid.Column="8"
												Width="200"
												Content="{Binding Supplier, Mode=TwoWay}"
												FontSize="15" />
											<Label
												Margin="5,10,0,0"
												Grid.Row="0"
												Grid.Column="9"
												Width="150"
												Content="{Binding OriginalPrice, Mode=TwoWay}"
												FontSize="15" />
											<Label
												Margin="5,10,0,0"
												Grid.Row="0"
												Grid.Column="10"
												Width="150"
												Content="{Binding RetailPrice, Mode=TwoWay}"
												FontSize="15" />
											<Label
												Margin="5,10,0,0"
												Grid.Row="0"
												Grid.Column="11"
												Width="200"
												Content="{Binding DateExpiration, Mode=TwoWay}"
												FontSize="15" />
											<Label
												Margin="5,10,0,0"
												Grid.Row="0"
												Grid.Column="12"
												Width="150"
												Content="{Binding DateofOrder, Mode=TwoWay}"
												FontSize="15" />
											<Label
												Margin="5,10,0,0"
												Grid.Row="0"
												Grid.Column="13"
												Width="1000"
												Content="{Binding Description, Mode=TwoWay}"
												FontSize="15" />
										</Grid>
									</DataTemplate>
								</ListBox.ItemTemplate>
								<ListBox.ItemContainerStyle>
									<Style
										TargetType="{x:Type ListBoxItem}">
										<EventSetter
											Event="MouseDoubleClick"
											Handler="listbox_Click1" />
									</Style>
								</ListBox.ItemContainerStyle>
							</ListBox>
						</Grid>
					</ScrollViewer>
				</TabItem>
				<TabItem
					Name="selectedd"
					Width="131"
					Margin="-5,-2,-2,0"
					Background="DarkGray"
					Height="50">
					<StackPanel
						Margin="0,-5,0,0"
						Height="367">
						<ScrollViewer
							HorizontalScrollBarVisibility="Auto"
							VerticalScrollBarVisibility="Auto"
							Height="367">
							<DataGrid
								Name="Sell_Product"
								AutoGenerateColumns="False"
								CanUserAddRows="False"
								Width="650"
								Height="270"
								Margin="0,-75,0,0">
								<DataGrid.Columns>
									<DataGridTemplateColumn
										Header="Image"
										Width="100">
										<DataGridTemplateColumn.CellTemplate>
											<DataTemplate>
												<Image
													Source="{Binding Image}"
													Height="80"
													Width="150" />
											</DataTemplate>
										</DataGridTemplateColumn.CellTemplate>
									</DataGridTemplateColumn>
									<DataGridTextColumn
										Header="Name"
										Width="Auto"
										Binding="{Binding Name}"
										IsReadOnly="True" />
									<DataGridTextColumn
										Header="Id Number"
										Width="Auto"
										Binding="{Binding IdNo}" />
									<DataGridTextColumn
										Header="Category"
										Width="Auto"
										Binding="{Binding Category}" />
									<DataGridTextColumn
										Header="Total Quantity Sell/Peso}"
										Width="Auto"
										Binding="{Binding totalsell}" />
									<DataGridTextColumn
										Header="Supplier"
										Width="Auto"
										Binding="{Binding Supplier}" />
									<DataGridTextColumn
										Header="Original Price/peso"
										Width="Auto"
										Binding="{Binding OriginalPrice}" />
									<DataGridTextColumn
										Header="Retail Price"
										Width="Auto"
										Binding="{Binding RetailPrice}" />
									<DataGridTextColumn
										Header="Total Sell Price/Peso"
										Width="Auto"
										Binding="{Binding Totalretail}" />
									<DataGridTextColumn
										Header="Brand"
										Width="Auto"
										Binding="{Binding Brand}" />
									<DataGridTextColumn
										Header="Date of Expiration"
										Width="Auto"
										Binding="{Binding DateExpiration}" />
									<DataGridTextColumn
										Header="Status"
										Width="Auto"
										Binding="{Binding Status}" />
									<DataGridTextColumn
										Header="Date Of Order"
										Width="Auto"
										Binding="{Binding DateofOrder}" />
									<DataGridTextColumn
										Header="Description"
										Width="Auto"
										Binding="{Binding Description}" />
								</DataGrid.Columns>
							</DataGrid>
						</ScrollViewer>
						<Grid>
							<Button
								Click="confirmsell_Click"
								Content="Confirm Sell"
								Grid.Column="0"
								Grid.Row="0"
								HorizontalAlignment="Stretch"
								VerticalAlignment="Stretch"
								Margin="300,-70,247,41" />
							<Button
								Click="removesel_Click"
								Content="Remove Selection"
								Grid.Column="0"
								Grid.Row="0"
								HorizontalAlignment="Stretch"
