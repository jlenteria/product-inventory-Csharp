								Click="clearsel_Click"
								Content="Clear Selection"
								Grid.Column="0"
								Grid.Row="0"
								HorizontalAlignment="Stretch"
								VerticalAlignment="Stretch"
								Margin="428,-70,126,41" />
						</Grid>
					</StackPanel>
					<TabItem.Header>
						<Label
							Content="Selected Product" />
					</TabItem.Header>
				</TabItem>
			</TabControl>
			<!-- return inventory -->
			<TabControl
				Background="Lavender"
				Name="return_tab"
				Margin="0,-540,0,0"
				VerticalAlignment="Stretch"
				HorizontalAlignment="Center"
				Grid.Row="0"
				Grid.Column="0"
				Height="450"
